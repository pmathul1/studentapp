<section class=" my-5"> 
    <article>
        <h1 class="text-center my-5">Student Data</h1>
<table class="table">
  <thead class="theadcustom text-white thead-white text-dark">
    <tr>
      <th scope="col">photo</th>   
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Date Of Birth</th>
      <th scope="col">Gender</th>
      <th scope="col">Education</th>
      <th scope="col">Skills</th>
      <th scope="col">Phone</th>
      <th scope="col">Email</th>
      <th scope="col">Location</th>
      <th scope="col">Percentage</th>
      <th scope="col"></th>
      
    </tr>
  </thead>
  <tbody class="tbodycustom text-dark">
      {{#each student}}
      <tr>
      {{!-- <th scope="row">1</th> --}}
      <td>  {{#if student_photo}}
            <img class="profilepic" src="{{splitUrl student_photo.[0].path}}" alt=""> </td>
            {{else}}
            <img class="profilepic" src="../../images/man.png" alt="">
            {{/if}}
      <td>{{student_id}} </td>
      <td>{{student_name}} </td>
      <td>{{student_dob}} </td>
      <td>{{student_gender}} </td>
      <td>{{student_education}} </td>
      <td>{{student_skills}} </td>
      <td>{{student_phone}} </td>
      <td>{{student_email}} </td>
      <td>{{student_location}} </td>
      <td>{{student_percentage}} </td>
      <td><a class="btn btn-success" href="/student/students/{{_id}}">Detail</a></td>
    </tr>
      {{else}}
    <tr>
        <td>No Data</td>
    </tr>
      {{/each}}
    
  </tbody>
</table>
    </article>
</section>